{
  "name": "mutant",
  "version": "0.0.1",
  "description": "A mutation testing framework for JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir .",
    "start": "node lib/index.js",
    "test": "mocha --recursive",
    "coverage": "istanbul cover _mocha -- --recursive",
    "watch": "babel src --out-dir . --watch"
  },
  "bin": {
    "mutant": "./bin/mutant.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benhartley/mutant.git"
  },
  "keywords": [
    "mutation",
    "testing",
    "test",
    "coverage"
  ],
  "author": "Ben Hartley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benhartley/mutant/issues"
  },
  "homepage": "https://github.com/benhartley/mutant#readme",
  "dependencies": {
    "async": "^1.5.2",
    "babel-generator": "^6.7.2",
    "babel-traverse": "^6.7.4",
    "babel-types": "^6.7.2",
    "babylon": "^6.7.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "figures": "^1.5.0",
    "lodash": "^4.6.1",
    "tap-parser": "^1.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rewire": "^2.5.1",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": "inline"
  }
}
